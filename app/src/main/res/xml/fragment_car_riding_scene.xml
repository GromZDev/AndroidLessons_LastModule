<?xml version="1.0" encoding="utf-8"?>
<MotionScene xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        android:rotation="360"
        motion:constraintSetStart="@id/start"
        motion:duration="10000">

        <OnClick
            motion:clickAction="toggle"
            motion:targetId="@id/myCar" />

        <KeyFrameSet>

            <KeyPosition
                motion:framePosition="5"
                motion:keyPositionType="pathRelative"
                motion:motionTarget="@id/myCar"
                motion:percentX="0.15"
                motion:percentY="0" />
            <KeyAttribute
                android:rotation="0"
                motion:framePosition="5"
                motion:motionTarget="@id/myCar" />
            <KeyAttribute
                android:rotation="-110"
                motion:framePosition="9"
                motion:motionTarget="@id/myCar" />

            <KeyPosition
                motion:framePosition="15"
                motion:keyPositionType="pathRelative"
                motion:motionTarget="@id/myCar"
                motion:percentX="0.25"
                motion:percentY="-0.24" />
            <KeyAttribute
                android:rotation="0"
                motion:framePosition="15"
                motion:motionTarget="@id/myCar" />
            <KeyAttribute
                android:rotation="40"
                motion:framePosition="18"
                motion:motionTarget="@id/myCar" />

            <KeyPosition
                motion:framePosition="20"
                motion:keyPositionType="pathRelative"
                motion:motionTarget="@id/myCar"
                motion:percentX="0.35"
                motion:percentY="-0.24" />
            <KeyAttribute
                android:rotation="40"
                motion:framePosition="20"
                motion:motionTarget="@id/myCar" />

            <KeyPosition
                motion:framePosition="25"
                motion:keyPositionType="pathRelative"
                motion:motionTarget="@id/myCar"
                motion:percentX="0.6"
                motion:percentY="-0.2" />
            <KeyAttribute
                android:rotation="70"
                motion:framePosition="25"
                motion:motionTarget="@id/myCar" />

            <KeyPosition
                motion:framePosition="30"
                motion:keyPositionType="pathRelative"
                motion:motionTarget="@id/myCar"
                motion:percentX="0.7"
                motion:percentY="0" />
            <KeyAttribute
                android:rotation="120"
                motion:framePosition="30"
                motion:motionTarget="@id/myCar" />

            <KeyPosition
                motion:framePosition="40"
                motion:keyPositionType="pathRelative"
                motion:motionTarget="@id/myCar"
                motion:percentX="0.6"
                motion:percentY="0.2" />
            <KeyAttribute
                android:rotation="170"
                motion:framePosition="40"
                motion:motionTarget="@id/myCar" />

            <KeyPosition
                motion:framePosition="50"
                motion:keyPositionType="pathRelative"
                motion:motionTarget="@id/myCar"
                motion:percentX="0.4"
                motion:percentY="0" />
            <KeyAttribute
                android:rotation="300"
                motion:framePosition="50"
                motion:motionTarget="@id/myCar" />

            <KeyPosition
                motion:framePosition="60"
                motion:keyPositionType="pathRelative"
                motion:motionTarget="@id/myCar"
                motion:percentX="0.6"
                motion:percentY="-0.2" />
            <KeyAttribute
                android:rotation="360"
                motion:framePosition="60"
                motion:motionTarget="@id/myCar" />
            <KeyAttribute
                android:rotation="390"
                motion:framePosition="80"
                motion:motionTarget="@id/myCar" />

            <KeyPosition
                motion:framePosition="90"
                motion:keyPositionType="pathRelative"
                motion:motionTarget="@id/myCar"
                motion:percentX="0.9"
                motion:percentY="0" />
            <KeyAttribute
                android:rotation="260"
                motion:framePosition="90"
                motion:motionTarget="@id/myCar" />

        </KeyFrameSet>

    </Transition>

    <ConstraintSet android:id="@+id/start">

        <Constraint
            android:id="@+id/myCar"
            android:layout_width="68dp"
            android:layout_height="68dp"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent">

            <CustomAttribute
                motion:attributeName="drive"
                motion:customFloatValue="0" />

        </Constraint>

    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">

        <Constraint
            android:id="@id/myCar"
            android:layout_width="68dp"
            android:layout_height="68dp"
            android:layout_marginStart="24dp"
            android:layout_marginTop="4dp"
            android:layout_marginEnd="24dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent">

            <CustomAttribute
                motion:attributeName="drive"
                motion:customFloatValue="1" />

        </Constraint>

    </ConstraintSet>
</MotionScene>